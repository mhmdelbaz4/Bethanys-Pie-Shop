@model IEnumerable<Pie>
@{
    ViewData["Title"] = "Home Page";
}


<partial name="_Carousel"></partial>

<h2>Pies of the week</h2>
<h4>Our weekly selection - just for you!</h4>
<div class="row">
    @foreach(var pie in Model)
    {
        <partial name="_PieCard" model="@pie"></partial>
    }
</div>